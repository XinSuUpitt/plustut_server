<% include header %>
	<table class="ui striped table">
	 	<thead>
	    	<tr>
		      	<th>学生ID</th>
				  <th>学生姓名</th>
				  <th>学生头像</th>
				  <th>学生邮箱</th>
				  <th>学生电话</th>
				  <th>学生微信</th>
				  
	    	</tr>
	  	</thead>
		<tbody>
		 <% users.forEach(function(user){ %>
			<tr>
	    		<td><%=  user.id %></td>
				<td><%=  user.name %></td>
	    		<td>
	    			<% if(user.avator!=''){ %>
	    				<img src="http://vue.wclimb.site/images/avator/<%=  user.avator %>.png" alt="">
	    			<% }else{ %>
	    				还没有上传头像！
	    			<% } %>
				</td>
				<td><%= user.email %></td>
				<td><%= user.phone_number %></td>
				<td><%= user.wechat %></td>
	    	</tr>
		 <% }) %>
			</tbody>
	</table>
	<%- include("pagination",{type:'mobileUser'}) %>
<% include footer %>          