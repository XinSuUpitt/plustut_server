<% include header %>
	<table class="ui striped table">
	 	<thead>
	    	<tr>
		      	<th>学生ID</th>
				  <th>学生姓名</th>
				  <th>学生电话</th>
				  <th>学生微信</th>
	    	</tr>
	  	</thead>
		<tbody>
		 <% users.forEach(function(user){ %>
			<tr>
	    		<td><%=  user.student_id %></td>
				<td><%=  user.student_name %></td>
				<td><%=  user.phone_number %></td>
				<td><%=  user.wechat %></td>
	    	</tr>
		 <% }) %>
		</tbody>
	</table>
	<%- include("pagination",{type:'adminUser'}) %>
<% include footer %>          